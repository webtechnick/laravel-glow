{
    "name": "webtechnick/laravel-glow",
    "description": "an army knife of tools for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick Baker",
            "email": "nick@webtechnick.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "intervention/image": "^2.3"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "autoload": {
        "psr-4": {
            "WebTechNick\\LaravelGlow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebTechNick\\LaravelGlow\\ServiceProvider"
            ]
        }
    }
}
